setenv loadaddr @UBOOT_LOADADDRESS@
setenv fdtaddr @UBOOT_FDTADDRESS@
setenv bootargs_usb setenv bootargs ${bootargs_base} ip=none root=/dev/@ROOT_DEV_NAME@@ROOT_DEV_SEP@2
setenv bootcmd_usb 'if ext2load usb 0:1 ${loadaddr} /@KERNEL_IMAGETYPE@; then run bootargs_usb; ext2load usb 0:1 ${fdtaddr} /${fdt_file}; bootz ${loadaddr} - ${fdtaddr}; fi'
setenv bootargs_rec setenv bootargs ${bootargs_base} ip=none root=/dev/@ROOT_DEV_NAME@@ROOT_DEV_SEP@3
setenv bootcmd_rec 'if ext2load usb 0:1 ${loadaddr} /@KERNEL_IMAGETYPE@-@MACHINE@; then run bootargs_rec; ext2load usb 0:1 ${fdtaddr} /${fdt_file}; bootz ${loadaddr} - ${fdtaddr}; fi'
usb dev 0
run bootcmd_rec
run bootcmd_usb
